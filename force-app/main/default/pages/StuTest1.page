<apex:page standardController="Student__c">
  <apex:sectionHeader title="Student Edit" subtitle="New Student"/>
  <apex:form >
    <apex:pageBlock title="Student Edit">
      <apex:pageBlockButtons >
        <apex:commandButton value="save" action="{!save}"/>
        <apex:commandButton value="save & New" />
        <apex:commandButton value="Cancel" />
      </apex:pageBlockButtons>
      <apex:pageBlockSection title="Student Information" columns="1">
        <apex:inputField value="{!Student__c.Name}"/>
        <apex:inputField value="{!Student__c.Last_Name__c}"/>        
        <apex:inputField value="{!Student__c.Batch__c}"/>
        <apex:inputField value="{!Student__c.University__c}"/>
        <apex:inputField value="{!Student__c.College__c}"/>
        <apex:inputField value="{!Student__c.Gender__c}"/>
      </apex:pageBlockSection>
    </apex:pageBlock>
  </apex:form>
</apex:page>